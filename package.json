    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.2",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
